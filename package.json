  "type": "module",
  "scripts": {
    "start": "tsx src/app.ts",
    "dev": "tsx watch --clear-screen=false src/app.ts",
    "build": "rimraf ./dist && tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "pnpm biome format --write ./src",
    "lint": "pnpm biome lint --write ./src",
    "check": "pnpm biome check --write ./src",
    "typeorm": "tsx ./node_modules/typeorm/cli.js",
    "m:gen": "pnpm typeorm migration:generate -d ./src/config/data-source.ts ./src/migrations/SugiereEntityChange",
    "m:run": "pnpm typeorm migration:run -d ./src/config/data-source.ts",
    "seed": "tsx src/database/seed.ts",
    "seed:user": "tsx src/database/seed.ts UserSeeder",
    "seed:role": "tsx src/database/seed.ts RoleSeeder",
    "seed:veterinarie_schedule_hours": "tsx src/database/seed.ts VeterinarianScheduleHoursSeeder"
  },
  "keywords": [],
